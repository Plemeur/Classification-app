{
    "dataset_path": "data/17flowers",
    "model_name": "resnet18",
    "pretrained": true,
    "loss_name":"categorical_crossentropy",
    "optimizer_name": "Adam",
    "optimizer_parameters": {},
    "scheduler_name": "StepLR",
    "scheduler_parameters": {
        "step_size" : 10,
        "gamma" : 0.1
    },
    "batch_size": 32,
    "num_workers": 8,
    "training_transforms": {
        "RandomAffine": {
            "degrees": 10,
            "translate": [
                0.1,
                0.1
            ],
            "scale": [
                0.80,
                1.20
            ],
            "fill": 255
        },
        "Resize": {
            "size": [
                224,
                224
            ]
        },
        "ToTensor": {}
    },
    "validation_transforms": {
        "Resize": {
            "size": [
                224,
                224
            ]
        },
        "ToTensor": {}
    }
}